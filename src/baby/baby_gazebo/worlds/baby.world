<?xml version="1.0" ?>
<sdf version="1.4">
  <!-- We use a custom world for the rrbot so that the camera angle is launched correctly -->

  <world name="default">
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <gravity>0.0 0 -60.0</gravity>
    <!-- Global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

  </world>
</sdf>
